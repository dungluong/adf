		{
			"name": "[concat(parameters('factoryName'), '/cleanData')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "AzureSqlTable1",
								"type": "DatasetReference"
							},
							"name": "dataCleaned"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "OutputCSV",
								"type": "DatasetReference"
							},
							"name": "sink1"
						}
					],
					"transformations": [
						{
							"name": "filter1"
						}
					],
					"scriptLines": [
						"source(output(",
						"          STT as string,",
						"          {Chỉ tiêu} as string,",
						"          {Tháng/ Quý này} as decimal(18,2),",
						"          { Tháng/ Quý trước } as decimal(18,2),",
						"          { Tháng/Quý cùng kỳ năm trước } as decimal(18,2),",
						"          { Kế hoạch Quý này } as decimal(18,2),",
						"          { Lũy kế đến Tháng/ Quý  này } as decimal(18,2),",
						"          { Lũy kế cùng kỳ năm trước } as decimal(18,2),",
						"          { Kế hoạch lũy kế đến Quý này } as decimal(18,2),",
						"          {Tăng trưởng so với tháng/quý trước} as decimal(5,2),",
						"          {Tăng trưởng so với tháng/quý cùng kỳ} as decimal(5,2),",
						"          {% /kế hoạch tháng/quý này} as decimal(5,2),",
						"          {Tăng trưởng lũy kế} as decimal(5,2),",
						"          {% kế hoạch lũy kế} as decimal(5,2)",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> dataCleaned",
						"dataCleaned filter({Chỉ tiêu} != 'Hàng hóa' && {Chỉ tiêu} != 'Dịch vụ') ~> filter1",
						"filter1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     partitionFileNames:['Output.csv'],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     saveOrder: 1,",
						"     partitionBy('hash', 1)) ~> sink1"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureSqlTable1')]",
				"[concat(variables('factoryId'), '/datasets/OutputCSV')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/sortData')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "AzureSqlTable1",
								"type": "DatasetReference"
							},
							"name": "sql"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "AzureSqlTable1",
								"type": "DatasetReference"
							},
							"name": "sink1"
						}
					],
					"transformations": [
						{
							"name": "sort1"
						},
						{
							"name": "filter1"
						},
						{
							"name": "derivedColumn1"
						}
					],
					"scriptLines": [
						"source(output(",
						"          STT as string,",
						"          {Chỉ tiêu} as string,",
						"          {Tháng/ Quý này} as decimal(18,2),",
						"          { Tháng/ Quý trước } as decimal(18,2),",
						"          { Tháng/Quý cùng kỳ năm trước } as decimal(18,2),",
						"          { Kế hoạch Quý này } as decimal(18,2),",
						"          { Lũy kế đến Tháng/ Quý  này } as decimal(18,2),",
						"          { Lũy kế cùng kỳ năm trước } as decimal(18,2),",
						"          { Kế hoạch lũy kế đến Quý này } as decimal(18,2),",
						"          {Tăng trưởng so với tháng/quý trước} as decimal(5,2),",
						"          {Tăng trưởng so với tháng/quý cùng kỳ} as decimal(5,2),",
						"          {% /kế hoạch tháng/quý này} as decimal(5,2),",
						"          {Tăng trưởng lũy kế} as decimal(5,2),",
						"          {% kế hoạch lũy kế} as decimal(5,2)",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> sql",
						"sql sort(desc({Tháng/ Quý này}, false)) ~> sort1",
						"sort1 filter({Tháng/ Quý này} != 0) ~> filter1",
						"filter1 derive({Tháng/ Quý Này Dương} = {Tháng/ Quý này} > 0) ~> derivedColumn1",
						"derivedColumn1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          STT as string,",
						"          {Chỉ tiêu} as string,",
						"          {Tháng/ Quý này} as decimal(18,2),",
						"          { Tháng/ Quý trước } as decimal(18,2),",
						"          { Tháng/Quý cùng kỳ năm trước } as decimal(18,2),",
						"          { Kế hoạch Quý này } as decimal(18,2),",
						"          { Lũy kế đến Tháng/ Quý  này } as decimal(18,2),",
						"          { Lũy kế cùng kỳ năm trước } as decimal(18,2),",
						"          { Kế hoạch lũy kế đến Quý này } as decimal(18,2),",
						"          {Tăng trưởng so với tháng/quý trước} as decimal(5,2),",
						"          {Tăng trưởng so với tháng/quý cùng kỳ} as decimal(5,2),",
						"          {% /kế hoạch tháng/quý này} as decimal(5,2),",
						"          {Tăng trưởng lũy kế} as decimal(5,2),",
						"          {% kế hoạch lũy kế} as decimal(5,2)",
						"     ),",
						"     deletable:false,",
						"     insertable:true,",
						"     updateable:false,",
						"     upsertable:false,",
						"     truncate:true,",
						"     format: 'table',",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureSqlTable1')]"
			]
		},
