{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "myazdfcmc"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase2'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"trigger1_properties_pipeline1_parameters_fileName": {
			"type": "string",
			"defaultValue": "CTS HN BCQT T2.2022-Edited.xlsx"
		},
		"trigger1_properties_pipeline1_parameters_tableName": {
			"type": "string",
			"defaultValue": "BCKD"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorage1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobStorage1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureSqlDatabase1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('AzureSqlDatabase1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureSqlDatabase2')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('AzureSqlDatabase2_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/SqlServer1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('SqlServer1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureSqlTable1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureSqlDatabase2",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"tableName": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "STT",
						"type": "nvarchar"
					},
					{
						"name": "Chỉ tiêu",
						"type": "nvarchar"
					},
					{
						"name": "Tháng/ Quý này",
						"type": "decimal",
						"precision": 18,
						"scale": 2
					},
					{
						"name": " Tháng/ Quý trước ",
						"type": "decimal",
						"precision": 18,
						"scale": 2
					},
					{
						"name": " Tháng/Quý cùng kỳ năm trước ",
						"type": "decimal",
						"precision": 18,
						"scale": 2
					},
					{
						"name": " Kế hoạch Quý này ",
						"type": "decimal",
						"precision": 18,
						"scale": 2
					},
					{
						"name": " Lũy kế đến Tháng/ Quý  này ",
						"type": "decimal",
						"precision": 18,
						"scale": 2
					},
					{
						"name": " Lũy kế cùng kỳ năm trước ",
						"type": "decimal",
						"precision": 18,
						"scale": 2
					},
					{
						"name": " Kế hoạch lũy kế đến Quý này ",
						"type": "decimal",
						"precision": 18,
						"scale": 2
					},
					{
						"name": "Tăng trưởng so với tháng/quý trước",
						"type": "decimal",
						"precision": 5,
						"scale": 2
					},
					{
						"name": "Tăng trưởng so với tháng/quý cùng kỳ",
						"type": "decimal",
						"precision": 5,
						"scale": 2
					},
					{
						"name": "% /kế hoạch tháng/quý này",
						"type": "decimal",
						"precision": 5,
						"scale": 2
					},
					{
						"name": "Tăng trưởng lũy kế",
						"type": "decimal",
						"precision": 5,
						"scale": 2
					},
					{
						"name": "% kế hoạch lũy kế",
						"type": "decimal",
						"precision": 5,
						"scale": 2
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": {
						"value": "@dataset().tableName",
						"type": "Expression"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureSqlDatabase2')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Excel1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureBlobStorage1",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"fileName": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "Excel",
				"typeProperties": {
					"sheetName": "1. BC KQKD CTY",
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": {
							"value": "@dataset().fileName",
							"type": "Expression"
						},
						"container": "mycontainer"
					},
					"firstRowAsHeader": true
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureBlobStorage1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "ExcelSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								}
							},
							"sink": {
								"type": "AzureSqlSink",
								"writeBehavior": "upsert",
								"upsertSettings": {
									"useTempDB": true,
									"keys": [
										"Chỉ tiêu"
									]
								},
								"sqlWriterUseTableLock": false,
								"disableMetricsCollection": false
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "STT",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "STT",
											"type": "String",
											"physicalType": "nvarchar"
										}
									},
									{
										"source": {
											"name": "Chỉ tiêu",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Chỉ tiêu",
											"type": "String",
											"physicalType": "nvarchar"
										}
									},
									{
										"source": {
											"name": "Tháng/ Quý này",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Tháng/ Quý này",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 18
										}
									},
									{
										"source": {
											"name": "Tháng/ Quý trước",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": " Tháng/ Quý trước ",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 18
										}
									},
									{
										"source": {
											"name": "Tháng/Quý cùng kỳ năm trước",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": " Tháng/Quý cùng kỳ năm trước ",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 18
										}
									},
									{
										"source": {
											"name": "Kế hoạch Quý này",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": " Kế hoạch Quý này ",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 18
										}
									},
									{
										"source": {
											"name": "Lũy kế đến Tháng/ Quý  này",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": " Lũy kế đến Tháng/ Quý  này ",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 18
										}
									},
									{
										"source": {
											"name": "Lũy kế cùng kỳ năm trước",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": " Lũy kế cùng kỳ năm trước ",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 18
										}
									},
									{
										"source": {
											"name": "Kế hoạch lũy kế đến Quý này",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": " Kế hoạch lũy kế đến Quý này ",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 18
										}
									},
									{
										"source": {
											"name": "Tăng trưởng so với tháng/quý trước",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Tăng trưởng so với tháng/quý trước",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 5
										}
									},
									{
										"source": {
											"name": "Tăng trưởng so với tháng/quý cùng kỳ",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Tăng trưởng so với tháng/quý cùng kỳ",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 5
										}
									},
									{
										"source": {
											"name": "% /kế hoạch tháng/quý này",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "% /kế hoạch tháng/quý này",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 5
										}
									},
									{
										"source": {
											"name": "Tăng trưởng lũy kế",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Tăng trưởng lũy kế",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 5
										}
									},
									{
										"source": {
											"name": "% kế hoạch lũy kế",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "% kế hoạch lũy kế",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 5
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "Excel1",
								"type": "DatasetReference",
								"parameters": {
									"fileName": {
										"value": "@pipeline().parameters.fileName",
										"type": "Expression"
									}
								}
							}
						],
						"outputs": [
							{
								"referenceName": "AzureSqlTable1",
								"type": "DatasetReference",
								"parameters": {
									"tableName": {
										"value": "@pipeline().parameters.tableName",
										"type": "Expression"
									}
								}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"parameters": {
					"fileName": {
						"type": "string",
						"defaultValue": "CTS HN BCQT T2.2022-Edited.xlsx"
					},
					"tableName": {
						"type": "string",
						"defaultValue": "BCKD"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/Excel1')]",
				"[concat(variables('factoryId'), '/datasets/AzureSqlTable1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/trigger1')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "pipeline1",
							"type": "PipelineReference"
						},
						"parameters": {
							"fileName": "[parameters('trigger1_properties_pipeline1_parameters_fileName')]",
							"tableName": "[parameters('trigger1_properties_pipeline1_parameters_tableName')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 3,
						"startTime": "2022-05-19T02:49:00Z",
						"timeZone": "UTC"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/pipeline1')]"
			]
		}
	]
}